{
    "manifest_version": 2,
    "name": "Petivity",
    "description": "Productiviy's Best Friend",
    "version": "1.0",
    "chrome_url_overrides" : {
        "newtab": "newTab.html"
    },
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html",
        "default_title": "Click here!"
    },
    "permissions": [
        "activeTab",
        "storage"
    ]
}
